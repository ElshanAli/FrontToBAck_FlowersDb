@{
    HomeViewModel homeViewModel = Model;
    var products = homeViewModel.Products;
}

<!-- PRODUCTS START -->

<section id="products">
    <div class="container">
       
        <div class="row">
            @foreach (var product in products)
            {
                @await Html.PartialAsync("_ProductPartialView", model:product)
            }

        </div>
    </div>
</section>

<!-- PRODUCTS END -->
